<Window x:Class="PendulumApp.Dialogs.DialogService.DialogWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PendulumApp.Dialogs.DialogService"
        mc:Ignorable="d"
        Title="{Binding DialogTittle}" Height="300" Width="300" ResizeMode="NoResize" WindowStartupLocation="CenterScreen"
         SizeToContent="WidthAndHeight">
    <ContentPresenter x:Name="ContentPresenter" Content="{Binding}"></ContentPresenter>
</Window>
